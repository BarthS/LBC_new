    .navbar.navbar-default.navbar-fixed-top{role: "navigation"}
      .container
        .navbar-header
          %button.navbar-toggle{"data-target" => ".navbar-responsive-collapse", "data-toggle" => "collapse", type: "button"}
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand{href: "#"}
            .glyphicon.glyphicon-chevron-left
            .glyphicon.glyphicon-shopping-cart
            .glyphicon.glyphicon-chevron-right
        .navbar-collapse.collapse.navbar-responsive-collapse
          %ul.nav.navbar-nav
            %li{id: "l1"}
              %a{href: "/page/index"} Accueil
            %li{id: "l2"}
              %a{href: "/cats/index"} Categories
            %li{id: "l3"}
              %a{href: "/profil/list"} Membres
            %li{id: "l4"}
              %a{href: "/reports/index"} Admin Panel
          .navbar-form.navbar-left
            .form-control
              = form_tag("/page/search", method: "get") do
                = text_field_tag :q, nil, placeholder: "Search"
          %p.navbar-text.pull-right
            - if user_signed_in?
              Logged in as
              = succeed "." do
                %strong= current_user.email
              = link_to 'Edit profile', edit_user_registration_path, :class => 'navbar-link'
              |
              \#{link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar-link'}
            - else
              = link_to "Sign up", new_user_registration_path, :class => 'navbar-link'
              |
              \#{link_to "Login", new_user_session_path, :class => 'navbar-link'}
            - if notice
              %p.navbar-text.pull-right= notice
            - if alert
              %p.navbar-text.pull-right= alert

    .navbar.navbar-default.navbar-static-top


